@model Typo.Model.Models.Score
<div class="page-header">
    <h1>ScoreTake <small>to take a score</small></h1>
</div>
<div class="container">
    <div class="row" id="test">
        <div class="col-sm-6">
            <div class="col-lg-12">
                <form method="post">
                    <div class="well">
                        <div class="form-group">
                            @Html.LabelFor(Model => Model.userId)
                            @Html.EditorFor(Model => Model.userId)
                            @Html.ValidationMessageFor(Model => Model.userId)
                        </div>
                        <button type="submit" class="btn btn-info">ScoreTake</button>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>
